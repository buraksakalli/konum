{
  "name": "konum",
  "version": "1.0.1",
  "author": "Burak SAKALLI <burak@buraksakalli.org>",
  "description": "A simple browser location hook for React",
  "license": "MIT",
  "main": "./dist/konum.umd.js",
  "module": "./dist/konum.es.js",
  "exports": {
    ".": {
      "import": "./dist/konum.es.js",
      "require": "./dist/konum.umd.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preci": "rm -rf node_modules",
    "ci": "yarn install --frozen-lockfile"
  },
  "dependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@types/node": "^17.0.27",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@vitejs/plugin-react": "^1.3.0",
    "typescript": "^4.6.3",
    "vite": "^2.9.5"
  },
  "homepage": "https://github.com/buraksakalli/konum",
  "keywords": [
    "react location",
    "react location hook",
    "react browser location"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:buraksakalli/konum.git"
  },
  "bugs": {
    "url": "https://github.com/buraksakalli/konum/issues"
